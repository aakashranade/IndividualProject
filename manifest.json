{
  "name": "PROV-extractor",
  "version": "0.2.0",
  "description": "Extract PROV data from news websites",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "PROV"
  },
  "permissions": ["tabs", "activeTab"],
  "manifest_version": 2
}
